<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/                 file:///usr/share/openbox/menu.xsd">
	<menu id="root-menu" label="Openbox 3">
		<item label="Terminator">
			<action name="Execute">
				<execute>terminator</execute>
			</action>
		</item>
		<item label="Google Chrome">
			<action name="Execute">
				<execute>env GTK2_RC_FILES=/usr/share/themes/Clearlooks/gtk-2.0/gtkrc google-chrome</execute>
			</action>
		</item>
		<item label="Icedove">
			<action name="Execute">
				<execute>env GTK2_RC_FILES=/usr/share/themes/Clearlooks/gtk-2.0/gtkrc icedove</execute>
			</action>
		</item>
		<item label="KeePass2">
			<action name="Execute">
				<execute>keepass2</execute>
			</action>
		</item>
		<item label="Spotify">
			<action name="Execute">
				<execute>spotify</execute>
			</action>
		</item>
		<!-- This requires the presence of the 'menu' package to work -->
		<menu id="/Debian"/>
		<separator/>
		<menu id="client-list-menu"/>
		<separator/>
		<item label="ObConf">
			<action name="Execute">
				<execute>obconf</execute>
			</action>
		</item>
		<item label="Reconfigure">
			<action name="Reconfigure"/>
		</item>
		<item label="Restart">
			<action name="Restart"/>
		</item>
		<separator/>
		<item label="Exit">
			<action name="Exit"/>
		</item>
	</menu>
</openbox_menu>
